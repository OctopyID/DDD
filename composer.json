{
	"name": "octopyid/laravel-ddd",
	"description": "Laravel Domain Driven Design",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Supian M",
			"email": "supianidz@gmail.com"
		}
	],
	"require": {
		"php": "^8.1"
	},
	"require-dev": {
		"orchestra/testbench": "^7.11"
	},
	"autoload": {
		"psr-4": {
			"Octopy\\DDD\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Octopy\\DDD\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Octopy\\DDD\\DDDServiceProvider"
			]
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
